@charset "UTF-8";
/* CSS Document */

/*Universal Style Rule */
*{
	margin: 0;
	padding: 0;
}
html
{
    overflow:hidden;
	width:520px;
}

body{
	background-color:#fff;	
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #464749;
	margin: 0;
	overflow:hidden;
	width:520px;
}

a{
	text-decoration:none;
	border: none;
	outline:none;
}
img{
	border:none:
}
a img{
	border: none;
}

.blueTxt{
	color: #054080;
	font-weight: bold;
}

#wrapper {
	width: 520px;
	margin: 0;
}

#header{	
	position:relative;
}

#header img{
	display: block;
}

#content{
	background:#fff url(../img/bg_repeat.jpg) repeat-y;
	padding: 5px 25px 20px 25px;
	height: 390px;
}

#form{
	background-image: url(../img/sup_bg_form.gif);
	background-repeat:no-repeat;
	float: left;
	padding: 15px 10px 0 10px;
	margin:0;	
	height: 375px;
	width: 245px;
}

#form input[type=text]{
	border: 1px solid #8a8a8b;
	margin: 0 0 6px 0;
	padding: 0 3px 0 3px;
	color: #464749;
}

#form form{
	margin: 8px 0 0 0;	
}
#form textarea{
	resize: none;
	padding: 0 3px 0 3px;
	margin:0;
}
#form .label{
	margin: 0 0px 0px 3px;
	padding:0;
}
#form select{
	margin: 2px 0 6px 0;
	padding: 3px 0 0 3px;
	border: solid 1px #8a8a8b; 
	background:#8a8a8b;
	color:#fff;
}
#form option { 
	background:#8a8a8b; 
}

#form .submitButton{
	margin-bottom:0;
	margin-top: 8px;
	text-align:center;
	border:none;
	outline:none;
}

#thankyou {
	position:absolute;
	top:-500px;
	left:25px;
	visibility:hidden;
}
#chat{
	text-align:right;
	background-image: url(../img/sup_bg_chat.gif);
	background-repeat:no-repeat;
	padding: 13px 0px 16px 0px;	
	text-align:center;
	height:249px;
	width: 200px;
	margin-left:270px;
}

#twitter{
	background-image: url(../img/sup_bg_twitter.gif);
	background-repeat:no-repeat;
	text-align:center;
	height: 111px;
	margin-left:269px;
	padding: 10px 0px 0 0px;
}

#visibleWindow{	
	position:absolute;
	overflow:hidden;
	width:355px;
	height:350px;
	}

#infoContainer{
	position:absolute;
	width: 3150px;
	left: -515px;
}
	.infoCard{
		width: 365px;
		height: 340px;
		float: left;
		padding: 10px 150px 0 0;
	}
	.vidLauncher{
		float: left;
		width: 151px;
		cursor:pointer;
	}
	.info{
		width: 200px;
		margin-left: 151px;	
	}
	.infoTitle {
		font-weight: bold;
		font-size: 18px;
		color: #054080;
		margin: 0 0 5px 7px;	
	}
	.tidbit{
		margin: 0 0 5px 7px;	
	}

.visibleWindow{
	position:relative;
	top: 15px;
	left: 360px;
	width:95px;
	height:240px;
	overflow:auto;
	max-width:95px;
	
}

.scroll-pane{
	position:relative;
	top: 15px;
	left: 360px;
	width:110px;
	height:220px;
	overflow:auto;	
	outline:none;
}

#scrollContainer{
	height: 430px;	
	width: 95px;
	outline:none;
}
	
	
